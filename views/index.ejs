<!DOCTYPE html>
<html lang="en">
	<%- include('./partial/header.ejs') %>
	<body>
		<%- include('./partial/nav.ejs') %>
		<div class="blog content">
			<h2>All blogs</h2>
			<% if (blogs.length > 0) { %> <% blogs.forEach(blog => { %>
			<h2><%=blog.title %></h2>
			<p><%=blog.snippet %></p>
			<%}) %> <% } else { %>
			<p>No blogs displayed</p>
			<% } %>
		</div>
		<%- include('./partial/footer.ejs') %>
	</body>
</html>
